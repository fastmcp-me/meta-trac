{
  "name": "wordpress-trac-mcp-server",
  "version": "1.0.0",
  "description": "Production-ready Model Context Protocol server for WordPress Trac integration on Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --compatibility-date 2024-09-23",
    "start": "wrangler dev --compatibility-date 2024-09-23",
    "deploy": "wrangler deploy --compatibility-date 2024-09-23",
    "deploy:core": "wrangler deploy --env core --compatibility-date 2024-09-23",
    "deploy:meta": "wrangler deploy --env meta --compatibility-date 2024-09-23",
    "deploy:plugins": "wrangler deploy --env plugins --compatibility-date 2024-09-23",
    "deploy:themes": "wrangler deploy --env themes --compatibility-date 2024-09-23",
    "deploy:bbpress": "wrangler deploy --env bbpress --compatibility-date 2024-09-23",
    "deploy:buddypress": "wrangler deploy --env buddypress --compatibility-date 2024-09-23",
    "deploy:glotpress": "wrangler deploy --env glotpress --compatibility-date 2024-09-23",
    "deploy:all": "npm run deploy:core && npm run deploy:meta && npm run deploy:plugins && npm run deploy:themes && npm run deploy:bbpress && npm run deploy:buddypress && npm run deploy:glotpress",
    "build": "echo 'Build handled by Wrangler'",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "type-check": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "test": "npm run type-check",
    "test:unit": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "wordpress",
    "trac",
    "cloudflare",
    "workers",
    "ai",
    "llm"
  ],
  "author": "James LePage",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@vercel/og": "^0.7.2",
    "agents": "^0.0.108",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@cloudflare/workers-types": "^4.20240529.0",
    "typescript": "^5.4.5",
    "vitest": "^2.1.9",
    "wrangler": "^4.25.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jameswlepage/trac-mcp"
  },
  "bugs": {
    "url": "https://github.com/Jameswlepage/trac-mcp/issues"
  },
  "homepage": "https://github.com/Jameswlepage/trac-mcp#readme"
}
